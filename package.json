{
  "name": "@greenruhm/connect",
  "version": "1.1.0",
  "description": "Greenruhm Connect is your white label, custom-branded connection to the music metaverse. It allows you to provide music NFT services inside your app with your branding.",
  "main": "src/index.js",
  "engines": {
    "npm": ">=6.0.0",
    "node": ">=14.0.0"
  },
  "scripts": {
    "dev": "NEXT_PUBLIC_GREENRUHM_URL=http://localhost:3001 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src && echo '-- linting finished --'",
    "prettier": "prettier --write 'src/**/*.js'",
    "test": "NODE_ENV=test node -r @babel/register -r @babel/polyfill src/test/unit.js | tap-nirvana",
    "pretty-quick": "pretty-quick --staged",
    "watch": "watch 'npm run -s test && npm run -s lint' src",
    "update": "updtr",
    "release": "standard-version && git push --follow-tags && npm publish"
  },
  "pre-commit": [
    "lint",
    "pretty-quick",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Greenruhm/connect.git"
  },
  "author": "Greenruhm, Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Greenruhm/connect/issues"
  },
  "homepage": "https://github.com/Greenruhm/connect#readme",
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.19.4",
    "@babel/preset-react": "7.18.6",
    "@babel/register": "7.18.9",
    "babel-plugin-module-resolver": "4.1.0",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "esm": "3.2.25",
    "next": "12.3.1",
    "nyc": "15.1.0",
    "pre-commit": "1.2.2",
    "prettier": "2.7.1",
    "pretty-quick": "3.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "resolve": "1.22.1",
    "riteway": "7.0.0",
    "standard-version": "9.5.0",
    "styled-jsx": "5.1.0",
    "tap-nirvana": "1.1.0",
    "tape": "5.6.1",
    "updtr": "4.0.0",
    "watch": "1.0.2"
  },
  "dependencies": {
    "@magic-ext/connect": "3.0.0",
    "@paralleldrive/feature-toggles": "1.0.4",
    "ethers": "5.7.2",
    "isomorphic-fetch": "3.0.0",
    "magic-sdk": "10.0.0"
  }
}
