{
  "name": "@greenruhm/connect",
  "version": "1.0.0",
  "description": "Greenruhm Connect is your white label, custom-branded connection to the music metaverse. It allows you to provide music NFT services inside your app with your branding.",
  "main": "src/index.js",
  "engines": {
    "npm": ">=6.0.0",
    "node": ">=14.0.0"
  },
  "scripts": {
    "dev": "NEXT_PUBLIC_GREENRUHM_URL=http://localhost:3001 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src && echo '-- linting finished --'",
    "prettier": "prettier --write 'src/**/*.js'",
    "test": "NODE_ENV=test node -r @babel/register -r @babel/polyfill src/test/unit.js | tap-nirvana",
    "pretty-quick": "pretty-quick --staged",
    "watch": "watch 'npm run -s test && npm run -s lint' src"
  },
  "pre-commit": [
    "lint",
    "pretty-quick",
    "test-unit"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Greenruhm/connect.git"
  },
  "author": "Greenruhm, Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Greenruhm/connect/issues"
  },
  "homepage": "https://github.com/Greenruhm/connect#readme",
  "devDependencies": {
    "@babel/core": "7.17.9",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/register": "7.17.7",
    "babel-plugin-module-resolver": "4.1.0",
    "cuid": "2.1.8",
    "eslint": "8.9.0",
    "eslint-config-prettier": "4.3.0",
    "esm": "3.2.25",
    "next": "12.2.0",
    "nyc": "15.1.0",
    "pre-commit": "1.2.2",
    "prettier": "1.19.1",
    "pretty-quick": "3.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "resolve": "1.22.0",
    "riteway": "6.3.1",
    "styled-jsx": "5.0.2",
    "tap-nirvana": "1.1.0",
    "tape": "5.5.2",
    "testcafe": "1.19.0",
    "watch": "1.0.2"
  },
  "dependencies": {
    "isomorphic-fetch": "3.0.0",
    "magic-sdk": "8.1.1"
  }
}
