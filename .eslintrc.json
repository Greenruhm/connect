{
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es2022": true
  },
  "extends": ["next/core-web-vitals", "eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "defaultParams": true,
      "destructuring": true,
      "modules": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["error", "unix"]
  }
}
